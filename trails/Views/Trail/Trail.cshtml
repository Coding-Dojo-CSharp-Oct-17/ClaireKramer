@{
    ViewData["Title"] = @ViewBag.TrailName;
}

<a href="/">Go Home</a>


<h1>@ViewBag.Trail.Name</h1>
<h3>Description:</h3>
<p>@ViewBag.Trail.Description</p>
<h3>Trail Length:</h3>
<p>@ViewBag.Trail.Length miles</p>
<h3>Elevation Change:</h3>
<p>@ViewBag.Trail.Elevation feet</p>
<p>Longitude - @ViewBag.Trail.Longitude</p>
<p>Latitude - @ViewBag.Trail.Latitude</p>

